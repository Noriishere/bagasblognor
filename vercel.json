{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/index",
      "destination": "/index.html"
    },
    {
      "source": "/profile",
      "destination": "/profile.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/index",
      "permanent": true
    },
    {
      "source": "/profile.html",
      "destination": "/profile",
      "permanent": true
    }
  ]
}
